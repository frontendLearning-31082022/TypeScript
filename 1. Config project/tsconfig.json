{
    "extends": "./tsconfig-checks.json",
    "compilerOptions": {
        "target": "es2021",
        "module": "es2015",
        "moduleResolution": "node",
        "allowJs": true,
        "outDir": "./public/build",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "sourceMap": true,
    },
    "exclude": [
        "./api_realestate/*",
        "./api_realestate2/*"
    ]
}