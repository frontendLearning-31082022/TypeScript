{
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.14.1",
        "@typescript-eslint/parser": "^7.14.1",
        "eslint": "^8.57.0",
        "http-server": "^14.1.1",
        "typescript": "^5.5.2"
    },
    "scripts": {
        "build": "tsc --build --clean && tsc",
        "build:dev": "tsc --build --clean && tsc -p tsconfig-dev.json",
        "start": "concurrently --kill-others \"rm -r -fo '.\\public\\build\\' || tsc --watch\"  \"http-server -c-1 -p 3000 \"",
        "lint": "eslint src --ext .js --ext .ts && tsc --noEmit",
        "lint-fix": "eslint src --ext .js --ext .ts --fix"
    }
}